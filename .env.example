# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Storage Bucket S3-Compatible Configuration
# These settings enable S3-compatible access to Supabase Storage
# Get these from Supabase Dashboard > Storage > Settings > S3 Connection
VITE_STORAGE_ACCESS_KEY=your_storage_access_key
VITE_STORAGE_SECRET_KEY=your_storage_secret_key
VITE_STORAGE_ENDPOINT=https://your_project_id.storage.supabase.co/storage/v1/s3
VITE_STORAGE_REGION=us-west-1
VITE_STORAGE_BUCKET_NAME=gmwb_public